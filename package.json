{
  "name": "nuclearest-js",
  "version": "0.0.1-alpha.0",
  "description": "A modern Javascript rest toolbox",
  "main": "lib/index.js",
  "publishConfig": {
    "registry": "https://npm.registry.com/"
  },
  "scripts": {
    "build": "scripty",
    "watch": "scripty",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "pretest": "standard ./src/*.js",
    "test": "nyc --cache  --silent --require babel-core/register mocha",
    "posttest": "nyc report --reporter lcov --reporter text"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harlott/nuclearest-js.git"
  },
  "keywords": [
    "fetch", "oauth token", "refresh token"
  ],
  "author": "harlott",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "es6-promise": "^4.1.0",
    "events": "^1.1.1",
    "fs-extra": "^2.1.2",
    "nyc": "^7.1.0",
    "query-string-object": "^0.2.5",
    "request": "^2.81.0",
    "scripty": "^1.7.0",
    "standard": "^7.1.2",
    "winston": "^2.3.1",
    "yargs": "^7.0.2"
  },
  "dependencies": {
    "fetch-ponyfill": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4"
  },
  "bin": {
    "apimock": "./bin/apimock.js"
  }
}
