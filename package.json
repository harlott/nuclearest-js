{
  "name": "nuclearest-js",
  "version": "0.7.1-alpha",
  "description": "A modern Javascript rest API toolbox",
  "publishConfig": {
    "registry": "https://npm.registry.com/"
  },
  "scripts": {
    "build": "scripty",
    "doc": "node .internal/build-doc github",
    "doc:site": "node .internal/build-doc site",
    "watch": "scripty",
    "test": "cross-env NODE_ENV=test nyc mocha -R spec --compilers js:babel-register  --recursive && nyc report --reporter=text-lcov | coveralls",
    "test:noCoverall": "cross-env NODE_ENV=test nyc mocha -R spec --compilers js:babel-register  --recursive",
    "test:watch": "npm test -- --watch ",
    "test:watchNoCoverall": "npm run test:noCoverall -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harlott/nuclearest-js.git"
  },
  "keywords": [
    "javascript",
    "ES6",
    "fetch",
    "WebStorage",
    "HTTP Request Headers",
    "oauth",
    "refresh token"
  ],
  "author": "harlott",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.22.1",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.3.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react-app": "^5.0.2",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.20.0",
    "babelify": "^10.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "connect": "^3.6.5",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "docdown": "^0.7.2",
    "es6-promise": "^4.1.0",
    "eslint": "^5.0.1",

    "events": "^3.0.0",
    "events": "^3.0.0",
    "fs-extra": "^7.0.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.1.1",
    "mochawesome": "^3.0.0",
    "nodemon": "^1.13.3",
    "nyc": "^12.0.1",
    "query-string-object": "^0.2.5",
    "request": "^2.81.0",
    "scripty": "^2.0.0",
    "standard": "^12.0.0",
    "transform-runtime": "0.0.0",
    "webpack": "^4.9.1",
    "winston": "^3.0.0",
    "yargs": "^12.0.1"
  },
  "dependencies": {
    "express": "^4.16.2",
    "fetch-ponyfill": "^6.0.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "require": "^2.4.20"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
